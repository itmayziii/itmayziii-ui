{
  "name": "itmayziii-ui",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "APP_ENV=development astro dev",
    "start": "APP_ENV=development astro dev",
    "build": "astro build",
    "build:dev": "APP_ENV=development astro build",
    "build:prod": "APP_ENV=production astro build",
    "preview": "astro preview",
    "preview:dev": "APP_ENV=development astro preview",
    "preview:prod": "APP_ENV=production astro preview",
    "astro": "astro",
    "introspectSchema": "graphql-inspector introspect http://localhost:3000/api/graphql --write schema.graphql",
    "lint": "eslint --ext \".ts, .js, .mjs, .astro\" src astro.config.mjs tailwind.config.cjs web-test-runner.config.mjs",
    "lint:fix": "npm run lint -- --fix",
    "typecheck": "tsc --noEmit",
    "test": "web-test-runner --config web-test-runner.config.mjs"
  },
  "dependencies": {
    "@astrojs/lit": "~3.0.2",
    "@astrojs/tailwind": "~5.0.0",
    "@webcomponents/template-shadowroot": "~0.2.1",
    "astro": "~3.0.13",
    "graphql": "~15.8.0",
    "graphql-tag": "~2.12.6",
    "lit": "~2.8.0",
    "tailwindcss": "~3.3.3"
  },
  "devDependencies": {
    "@astrojs/ts-plugin": "~1.1.3",
    "@graphql-codegen/cli": "~4.0.1",
    "@graphql-inspector/cli": "~4.0.2",
    "@open-wc/testing": "~3.2.0",
    "@typescript-eslint/eslint-plugin": "~6.5.0",
    "@typescript-eslint/parser": "~6.6.0",
    "@web/dev-server-esbuild": "~0.4.3",
    "@web/test-runner": "~0.17.2",
    "commitizen": "~4.3.0",
    "cz-conventional-changelog": "~3.3.0",
    "eslint": "~8.48.0",
    "eslint-config-standard-with-typescript": "~39.0.0",
    "eslint-plugin-astro": "~0.29.0",
    "eslint-plugin-import": "~2.28.1",
    "eslint-plugin-n": "~15.7.0",
    "eslint-plugin-promise": "~6.1.1",
    "eslint-plugin-tsdoc": "~0.2.17",
    "typescript": "~5.2.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
